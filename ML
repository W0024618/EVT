# List installed Python launchers (Windows)
py -0p

# Or check default python on PATH
python --version
where.exe python







# deactivate if active
deactivate 2>$null

# remove the broken virtualenv folder (will delete .venv)
Remove-Item -Recurse -Force .\.venv





# Activate
.\.venv\Scripts\Activate.ps1

# Now prompt should show '(.venv)'




python -m pip install --upgrade pip setuptools wheel
# if you have requirements.txt
python -m pip install -r requirements.txt

# otherwise, if there is no requirements file, install the project's typical deps:
# pip install uvicorn fastapi  (and any other libs your app uses)




python -m pip install fastapi uvicorn





# from the same folder; app:app assumes your ASGI app object is named `app` in app.py
python -m uvicorn app:app --host 0.0.0.0 --port 8000

# or
uvicorn app:app --host 0.0.0.0 --port 8000












