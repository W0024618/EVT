SELECT Distinct
    AP.ObjectID AS "AccountName",
    AP.Name AS "Emp Name",
    CASE AP.Int1
        WHEN 0 THEN AP.Text12
        ELSE CAST(AP.Int1 AS NVARCHAR)
    END AS "EmployeeID",
 AP.text9 AS Temp_Badge_No,
    PT.Name AS "PersonnelType",
 AP.Text10 AS Manager_Name,
 AP.Int4 As Manager_WU_ID,
 AC.Name AS  Clearance,
null as "Extend_Replace"
FROM
    ACVSCore.Access.PersonnelClearancePair APC   
INNER JOIN ACVSCore.Access.Personnel AP
    ON AP.ObjectID = APC.PersonnelID
INNER JOIN ACVSCore.Access.Clearance AC
    ON AC.ObjectID = APC.ClearanceID  
INNER JOIN ACVSCore.Access.PersonnelType PT
    ON PT.ObjectID = AP.PersonnelTypeID
--WHERE AP.Disabled = 0
ORDER BY
    "EmployeeID" ASC, AC.Name;



