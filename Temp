psql -h your_pg_host -U postgres
# then inside psql:
CREATE DATABASE attendance_db;
CREATE USER att_user WITH PASSWORD 'strongpassword';
GRANT ALL PRIVILEGES ON DATABASE attendance_db TO att_user;



$env:ATT_DB_URL = "postgresql://att_user:strongpassword@your_pg_host:5432/attendance_db"



python create_tables.py









