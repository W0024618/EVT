# tools/sqlite_iterdump.py
import sqlite3, sys
fn = "attendance.db"
out = "dump_iter.sql"
try:
    conn = sqlite3.connect(fn)
    with open(out, "w", encoding="utf-8") as f:
        for line in conn.iterdump():
            f.write("%s\n" % line)
    print("iterdump wrote", out)
    conn.close()
except Exception as e:
    print("iterdump failed:", e)
    sys.exit(1)





(.venv) python tools\sqlite_iterdump.py









