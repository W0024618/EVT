cd C:\projects\yolo_project
.\.venv\Scripts\Activate.ps1
# prompt should now start with (.venv)





pwd
Get-ChildItem .\yolov8n.pt -Force






# from inside C:\projects\yolo_project with venv active
Move-Item C:\projects\yolov8n.pt .\
# verify
Get-ChildItem .\yolov8n.pt





Invoke-WebRequest -Uri "https://ultralytics.com/images/bus.jpg" -OutFile .\test_bus.jpg -UseBasicParsing -TimeoutSec 120
Get-ChildItem .\test_bus.jpg







# Python script method
python .\detect.py

# OR (CLI method) - either works when venv is active
yolo detect predict model=.\yolov8n.pt source=.\test_bus.jpg
# or using remote image (no local test image needed)
yolo detect predict model=.\yolov8n.pt source=https://ultralytics.com/images/bus.jpg







ii (Get-ChildItem -Directory .\runs\detect | Sort-Object LastWriteTime -Descending | Select-Object -First 1).FullName









